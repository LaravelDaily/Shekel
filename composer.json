{
  "name": "shekel/shekel",
  "type": "project",
  "license": "MIT",
  "description": "Payment package for laravel applications",
  "authors": [
    {
      "name": "Karolis",
      "email": "narkevicius.karolis@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "stripe/stripe-php": "^7.13",
    "paypal/rest-api-sdk-php": "^1.14"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^3.8|^4.0|^5.0",
    "phpunit/phpunit": "^7.5|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Shekel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Shekel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shekel\\ShekelServiceProvider"
      ]
    }
  }
}
